@model IHMS.Models.Admin
@{
    ViewBag.Title = "Registration";
    Layout = null;
}

<!doctype html>

<html class="no-js " lang="en">

<!-- Mirrored from wrraptheme.com/templates/aero/html/sign-up.html by HTTrack Website Copier/3.x [XR&CO'2014], Sat, 02 Apr 2022 06:54:53 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <meta name="description" content="Responsive Bootstrap 4 and web Application ui kit.">

    <title>:: Aero Bootstrap4 Admin :: Sign Up</title>
    <!-- Favicon-->
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <!-- Custom Css -->
    <link rel="stylesheet" href="~/assets/plugins/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/style.min.css">
</head>

<body class="theme-blush" id="myfrm">

    <div class="authentication">
        <div class="container">
            <div class="row" style="margin-top:13rem!important">
                <div class="col-lg-8 col-sm-12">
                    <div class="card auth_form">
                        <div class="header">
                            <img class="logo" src="~/assets/images/logo.svg" alt="">
                            <h5>Sign Up</h5>
                            <span>Register a new membership</span>
                        </div>
                        <div class="body">

                            <div class="input-group mb-3">
                                @*<label class="mb-3"><strong>Name</strong></label>*@
                                <input type="text" onchange="validdate('#txtname','#msgName','Name ');" id="txtname" class="form-control" placeholder="Username">
                                <div class="input-group-append">
                                    <span class="input-group-text text-danger"><i class="zmdi zmdi-account-circle"></i></span>
                                </div>
                            </div>
                            <span id="msgName" class="text-danger"></span>
                            <div class="input-group mb-3">
                                @*<label class="mb-3"><strong>Email</strong></label>*@
                                <input type="email" onchange="validdate('#txtemail','#email','Email ');" id="txtemail" class="form-control" placeholder="Enter Email">
                                <div class="input-group-append">
                                    <span class="input-group-text text-danger"><i class="zmdi zmdi-email"></i></span>
                                </div>
                            </div>
                            <span id="email" class="text-danger"></span>
                            <div class="input-group mb-3">
                                @*<label class="mb-3"><strong>Contact</strong></label>*@
                                <input type="number" onchange="validdate('#txtcontact','#contact','Contact')" id="txtcontact" class="form-control" placeholder="Contact">
                                <div class="input-group-append">
                                    <span class="input-group-text text-danger"><i class="zmdi zmdi-confirmation-number"></i></span>
                                </div>
                            </div>
                            <span id="contact" class="text-danger"></span>
                            <div class="input-group mb-3">
                                @*<label class="mb-3"><strong>CNIC</strong></label>*@
                                <input type="number" onchange="validdate('#txtcnic','#cnic','CNIC')" id="txtcnic" class="form-control" placeholder="CNIC">
                                <div class="input-group-append">
                                    <span class="input-group-text text-danger"><i class="zmdi zmdi-text-format"></i></span>
                                </div>
                            </div>
                            <span id="cnic" class="text-danger"></span>
                            <div class="input-group mb-3">
                                @*<label class="mb-3"><strong>Address</strong></label>*@
                                <input type="text" onchange="validdate('#txtaddress','#address','Address')" id="txtaddress" class="form-control" placeholder="Address">
                                <div class="input-group-append">
                                    <span class="input-group-text text-danger"><i class="zmdi zmdi-text-format"></i></span>
                                </div>
                            </div>
                            <span id="address" class="text-danger"></span>
                            <div class="input-group mb-3">
                                @*<label class="mb-3"><strong>Password</strong></label>*@
                                <input type="password" onchange="validdate('#txtpassword','#password','Password')" id="txtpassword" class="form-control" placeholder="Password">
                                <div class="input-group-append">
                                    <span class="input-group-text text-danger"><i class="zmdi zmdi-lock"></i></span>
                                </div>
                            </div>
                            <span id="password" class="text-danger"></span>
                            <div class="input-group mb-3">
                                @*<label class="mb-3"><strong>Image</strong></label>*@
                                <input type="file" onchange="validdate('#txtImage','#image','Image')" id="txtImage" class="form-control">
                                <div class="input-group-append">
                                    <span class="input-group-text text-danger"><i class="zmdi zmdi-image"></i></span>
                                </div>
                            </div>
                            <span id="image" class="text-danger"></span>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Status</label>
                                    <select class="form-control" onchange="validdateDropDowns('#ddlstatus', '#msgStatus', 'Status');" id="ddlstatus">
                                        <option value="-1">----Select Status----</option>
                                        <option value="1">Active</option>
                                        <option value="0">InActive</option>
                                    </select>
                                    <span class="text-danger" id="msgStatus"></span>
                                </div>
                            </div>
                            <div class="text-center mt-4">
                                <button id="btnSubmit" onclick="RegisterAdmin();" class="btn btn-success btn-block"><h5>Sign me up</h5></button>
                            </div>
                            <div class="signin_with mt-3">
                                <a class="link" href="@Url.Action("LogIn","Account")">You already have a membership?</a>
                            </div>
                        </div>
                    </div>
                    <div class="copyright text-center">
                        &copy;
                        <script>document.write(new Date().getFullYear())</script>,
                        <span>Designed by <a href="https://thememakker.com/" target="_blank">ThemeMakker</a></span>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-12">
                    <div class="card">
                        <img src="~/assets/images/signup.svg" alt="Sign Up" />
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!-- Jquery Core Js -->
    <script src="~/assets/bundles/libscripts.bundle.js"></script>
    <script src="~/assets/bundles/vendorscripts.bundle.js"></script> <!-- Lib Scripts Plugin Js -->

    <script type="text/javascript" src="~/Scripts/jquery-3.4.1.js"></script>
    <link href="~/IHMSJS/iziToast.css" rel="stylesheet" />
    <script src="~/IHMSJS/iziToast.js"></script>
    <script src="~/IHMSJS/AdminJS/Account.js"></script>

</body>

